{
  "openapi": "3.1.0",
  "info": {
    "title": "m5chat REST API",
    "version": "1.0.0",
    "description": "Spec (workspace auth, providers at workspace level)"
  },
  "servers": [
    {
      "url": "http://localhost:5179"
    }
  ],
  "paths": {
    "/api/health": {
      "get": {
        "summary": "Health check",
        "parameters": [
          {
            "$ref": "#/components/parameters/SessionQuery"
          }
        ],
        "responses": {
          "200": {
            "description": "Health response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HealthResponse"
                }
              }
            }
          }
        }
      }
    },
    "/api/session/{sessionId}": {
      "get": {
        "summary": "Get session",
        "parameters": [
          {
            "$ref": "#/components/parameters/SessionId"
          }
        ],
        "responses": {
          "200": {
            "description": "Session state",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/SessionResponse"
                }
              }
            }
          },
          "404": {
            "$ref": "#/components/responses/NotFound"
          }
        }
      }
    },
    "/api/session/{sessionId}/diff": {
      "get": {
        "summary": "Get repo diff for session",
        "parameters": [
          {
            "$ref": "#/components/parameters/SessionId"
          }
        ],
        "responses": {
          "200": {
            "description": "Repo diff",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/RepoDiff"
                }
              }
            }
          },
          "404": {
            "$ref": "#/components/responses/NotFound"
          }
        }
      }
    },
    "/api/session/{sessionId}/clear": {
      "post": {
        "summary": "Clear messages for provider or worktree",
        "parameters": [
          {
            "$ref": "#/components/parameters/SessionId"
          }
        ],
        "requestBody": {
          "required": false,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/SessionClearRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Cleared",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/OkResponse"
                }
              }
            }
          },
          "404": {
            "$ref": "#/components/responses/NotFound"
          }
        }
      }
    },
    "/api/session": {
      "post": {
        "summary": "Create session",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CreateSessionRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Session created",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/CreateSessionResponse"
                }
              }
            }
          },
          "400": {
            "$ref": "#/components/responses/BadRequest"
          },
          "500": {
            "$ref": "#/components/responses/ServerError"
          }
        }
      }
    },
    "/api/branches": {
      "get": {
        "summary": "List branches",
        "parameters": [
          {
            "$ref": "#/components/parameters/SessionQuery"
          }
        ],
        "responses": {
          "200": {
            "description": "Branch info",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/BranchInfo"
                }
              }
            }
          },
          "400": {
            "$ref": "#/components/responses/BadRequest"
          }
        }
      }
    },
    "/api/models": {
      "get": {
        "summary": "List models",
        "parameters": [
          {
            "$ref": "#/components/parameters/SessionQuery"
          },
          {
            "$ref": "#/components/parameters/ProviderQuery"
          }
        ],
        "responses": {
          "200": {
            "description": "Model list",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ModelListResponse"
                }
              }
            }
          },
          "400": {
            "$ref": "#/components/responses/BadRequest"
          },
          "403": {
            "$ref": "#/components/responses/Forbidden"
          },
          "404": {
            "$ref": "#/components/responses/NotFound"
          }
        }
      }
    },
    "/api/branches/switch": {
      "post": {
        "summary": "Switch branch",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/SwitchBranchRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Branch info",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/BranchInfo"
                }
              }
            }
          },
          "400": {
            "$ref": "#/components/responses/BadRequest"
          },
          "404": {
            "$ref": "#/components/responses/NotFound"
          },
          "409": {
            "$ref": "#/components/responses/Conflict"
          },
          "500": {
            "$ref": "#/components/responses/ServerError"
          }
        }
      }
    },
    "/api/worktrees": {
      "get": {
        "summary": "List worktrees",
        "parameters": [
          {
            "$ref": "#/components/parameters/SessionQuery"
          }
        ],
        "responses": {
          "200": {
            "description": "Worktree list",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/WorktreeListResponse"
                }
              }
            }
          },
          "400": {
            "$ref": "#/components/responses/BadRequest"
          }
        }
      }
    },
    "/api/worktree": {
      "post": {
        "summary": "Create worktree",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CreateWorktreeRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Worktree created",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/CreateWorktreeResponse"
                }
              }
            }
          },
          "400": {
            "$ref": "#/components/responses/BadRequest"
          },
          "500": {
            "$ref": "#/components/responses/ServerError"
          }
        }
      }
    },
    "/api/worktree/{worktreeId}": {
      "get": {
        "summary": "Get worktree",
        "parameters": [
          {
            "$ref": "#/components/parameters/WorktreeId"
          },
          {
            "$ref": "#/components/parameters/SessionQuery"
          }
        ],
        "responses": {
          "200": {
            "description": "Worktree detail",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/WorktreeDetail"
                }
              }
            }
          },
          "400": {
            "$ref": "#/components/responses/BadRequest"
          },
          "404": {
            "$ref": "#/components/responses/NotFound"
          }
        }
      },
      "delete": {
        "summary": "Delete worktree",
        "parameters": [
          {
            "$ref": "#/components/parameters/WorktreeId"
          },
          {
            "$ref": "#/components/parameters/SessionQuery"
          }
        ],
        "responses": {
          "200": {
            "description": "Worktree removed",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/OkResponse"
                }
              }
            }
          },
          "400": {
            "$ref": "#/components/responses/BadRequest"
          },
          "500": {
            "$ref": "#/components/responses/ServerError"
          }
        }
      },
      "patch": {
        "summary": "Rename worktree",
        "parameters": [
          {
            "$ref": "#/components/parameters/WorktreeId"
          },
          {
            "$ref": "#/components/parameters/SessionQuery"
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/RenameWorktreeRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Worktree updated",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/WorktreeRenameResponse"
                }
              }
            }
          },
          "400": {
            "$ref": "#/components/responses/BadRequest"
          },
          "404": {
            "$ref": "#/components/responses/NotFound"
          }
        }
      }
    },
    "/api/worktree/{worktreeId}/tree": {
      "get": {
        "summary": "Get directory tree",
        "parameters": [
          {
            "$ref": "#/components/parameters/WorktreeId"
          },
          {
            "$ref": "#/components/parameters/SessionQuery"
          },
          {
            "$ref": "#/components/parameters/TreeDepth"
          },
          {
            "$ref": "#/components/parameters/TreeLimit"
          }
        ],
        "responses": {
          "200": {
            "description": "Directory tree",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/DirectoryTree"
                }
              }
            }
          },
          "400": {
            "$ref": "#/components/responses/BadRequest"
          },
          "404": {
            "$ref": "#/components/responses/NotFound"
          }
        }
      }
    },
    "/api/worktree/{worktreeId}/file": {
      "get": {
        "summary": "Read file",
        "parameters": [
          {
            "$ref": "#/components/parameters/WorktreeId"
          },
          {
            "$ref": "#/components/parameters/SessionQuery"
          },
          {
            "$ref": "#/components/parameters/FilePath"
          }
        ],
        "responses": {
          "200": {
            "description": "File content",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/FileReadResponse"
                }
              }
            }
          },
          "400": {
            "$ref": "#/components/responses/BadRequest"
          },
          "404": {
            "$ref": "#/components/responses/NotFound"
          }
        }
      },
      "post": {
        "summary": "Write file",
        "parameters": [
          {
            "$ref": "#/components/parameters/WorktreeId"
          },
          {
            "$ref": "#/components/parameters/SessionQuery"
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/FileWriteRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Write ok",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/FileWriteResponse"
                }
              }
            }
          },
          "400": {
            "$ref": "#/components/responses/BadRequest"
          },
          "404": {
            "$ref": "#/components/responses/NotFound"
          }
        }
      }
    },
    "/api/worktree/{worktreeId}/status": {
      "get": {
        "summary": "Worktree git status",
        "parameters": [
          {
            "$ref": "#/components/parameters/WorktreeId"
          },
          {
            "$ref": "#/components/parameters/SessionQuery"
          }
        ],
        "responses": {
          "200": {
            "description": "Status entries",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/WorktreeStatusResponse"
                }
              }
            }
          },
          "400": {
            "$ref": "#/components/responses/BadRequest"
          },
          "404": {
            "$ref": "#/components/responses/NotFound"
          }
        }
      }
    },
    "/api/worktree/{worktreeId}/diff": {
      "get": {
        "summary": "Worktree diff",
        "parameters": [
          {
            "$ref": "#/components/parameters/WorktreeId"
          },
          {
            "$ref": "#/components/parameters/SessionQuery"
          }
        ],
        "responses": {
          "200": {
            "description": "Worktree diff",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/RepoDiff"
                }
              }
            }
          },
          "400": {
            "$ref": "#/components/responses/BadRequest"
          }
        }
      }
    },
    "/api/worktree/{worktreeId}/commits": {
      "get": {
        "summary": "Worktree commits",
        "parameters": [
          {
            "$ref": "#/components/parameters/WorktreeId"
          },
          {
            "$ref": "#/components/parameters/SessionQuery"
          },
          {
            "$ref": "#/components/parameters/CommitsLimit"
          }
        ],
        "responses": {
          "200": {
            "description": "Commit list",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/CommitListResponse"
                }
              }
            }
          },
          "400": {
            "$ref": "#/components/responses/BadRequest"
          }
        }
      }
    },
    "/api/worktree/{worktreeId}/merge": {
      "post": {
        "summary": "Merge worktree into target worktree",
        "parameters": [
          {
            "$ref": "#/components/parameters/WorktreeId"
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/MergeWorktreeRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Merge result",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/MergeWorktreeResponse"
                }
              }
            }
          },
          "400": {
            "$ref": "#/components/responses/BadRequest"
          },
          "500": {
            "$ref": "#/components/responses/ServerError"
          }
        }
      }
    },
    "/api/worktree/{worktreeId}/abort-merge": {
      "post": {
        "summary": "Abort merge",
        "parameters": [
          {
            "$ref": "#/components/parameters/WorktreeId"
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/SessionOnlyRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Aborted",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/OkResponse"
                }
              }
            }
          },
          "400": {
            "$ref": "#/components/responses/BadRequest"
          },
          "500": {
            "$ref": "#/components/responses/ServerError"
          }
        }
      }
    },
    "/api/worktree/{worktreeId}/cherry-pick": {
      "post": {
        "summary": "Cherry-pick commit",
        "parameters": [
          {
            "$ref": "#/components/parameters/WorktreeId"
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CherryPickRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Cherry-pick result",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/MergeWorktreeResponse"
                }
              }
            }
          },
          "400": {
            "$ref": "#/components/responses/BadRequest"
          },
          "500": {
            "$ref": "#/components/responses/ServerError"
          }
        }
      }
    },
    "/api/attachments": {
      "get": {
        "summary": "List attachments",
        "parameters": [
          {
            "$ref": "#/components/parameters/SessionQuery"
          }
        ],
        "responses": {
          "200": {
            "description": "Attachment list",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/AttachmentListResponse"
                }
              }
            }
          },
          "400": {
            "$ref": "#/components/responses/BadRequest"
          }
        }
      }
    },
    "/api/attachments/file": {
      "get": {
        "summary": "Download attachment",
        "parameters": [
          {
            "$ref": "#/components/parameters/SessionQuery"
          },
          {
            "$ref": "#/components/parameters/AttachmentPath"
          },
          {
            "$ref": "#/components/parameters/AttachmentName"
          }
        ],
        "responses": {
          "200": {
            "description": "Attachment file"
          },
          "400": {
            "$ref": "#/components/responses/BadRequest"
          },
          "404": {
            "$ref": "#/components/responses/NotFound"
          }
        }
      }
    },
    "/api/attachments/upload": {
      "post": {
        "summary": "Upload attachments",
        "parameters": [
          {
            "$ref": "#/components/parameters/SessionQuery"
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "files": {
                    "type": "array",
                    "items": {
                      "type": "string",
                      "format": "binary"
                    }
                  }
                },
                "required": [
                  "files"
                ]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Uploaded",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/AttachmentUploadResponse"
                }
              }
            }
          },
          "400": {
            "$ref": "#/components/responses/BadRequest"
          }
        }
      }
    },
    "/api/workspaces": {
      "post": {
        "summary": "Create workspace",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/WorkspaceCreateRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Workspace created",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/WorkspaceCreateResponse"
                }
              }
            }
          },
          "400": {
            "$ref": "#/components/responses/BadRequest"
          },
          "500": {
            "$ref": "#/components/responses/ServerError"
          }
        },
        "security": []
      }
    },
    "/api/workspaces/login": {
      "post": {
        "summary": "Login workspace (exchange secret for token)",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/WorkspaceLoginRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Workspace token",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/WorkspaceLoginResponse"
                }
              }
            }
          },
          "400": {
            "$ref": "#/components/responses/BadRequest"
          },
          "403": {
            "$ref": "#/components/responses/Forbidden"
          }
        },
        "security": []
      }
    },
    "/api/workspaces/{workspaceId}": {
      "delete": {
        "summary": "Delete workspace (policy TBD)",
        "parameters": [
          {
            "$ref": "#/components/parameters/WorkspaceId"
          }
        ],
        "responses": {
          "200": {
            "description": "Workspace deleted",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/OkResponse"
                }
              }
            }
          },
          "400": {
            "$ref": "#/components/responses/BadRequest"
          },
          "403": {
            "$ref": "#/components/responses/Forbidden"
          },
          "500": {
            "$ref": "#/components/responses/ServerError"
          }
        }
      },
      "patch": {
        "summary": "Update workspace providers/auth",
        "parameters": [
          {
            "$ref": "#/components/parameters/WorkspaceId"
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/WorkspaceUpdateRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Workspace updated",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/WorkspaceUpdateResponse"
                }
              }
            }
          },
          "400": {
            "$ref": "#/components/responses/BadRequest"
          },
          "403": {
            "$ref": "#/components/responses/Forbidden"
          },
          "500": {
            "$ref": "#/components/responses/ServerError"
          }
        }
      }
    }
  },
  "components": {
    "parameters": {
      "SessionQuery": {
        "name": "session",
        "in": "query",
        "required": false,
        "schema": {
          "type": "string"
        }
      },
      "SessionId": {
        "name": "sessionId",
        "in": "path",
        "required": true,
        "schema": {
          "type": "string",
          "pattern": "^s[0-9a-f]{24}$"
        }
      },
      "WorktreeId": {
        "name": "worktreeId",
        "in": "path",
        "required": true,
        "schema": {
          "type": "string"
        }
      },
      "ProviderQuery": {
        "name": "provider",
        "in": "query",
        "required": true,
        "schema": {
          "$ref": "#/components/schemas/Provider"
        }
      },
      "TreeDepth": {
        "name": "depth",
        "in": "query",
        "required": false,
        "schema": {
          "type": "integer",
          "minimum": 0
        }
      },
      "TreeLimit": {
        "name": "limit",
        "in": "query",
        "required": false,
        "schema": {
          "type": "integer",
          "minimum": 1
        }
      },
      "FilePath": {
        "name": "path",
        "in": "query",
        "required": true,
        "schema": {
          "type": "string"
        }
      },
      "CommitsLimit": {
        "name": "limit",
        "in": "query",
        "required": false,
        "schema": {
          "type": "integer",
          "minimum": 1
        }
      },
      "AttachmentPath": {
        "name": "path",
        "in": "query",
        "required": false,
        "schema": {
          "type": "string"
        }
      },
      "AttachmentName": {
        "name": "name",
        "in": "query",
        "required": false,
        "schema": {
          "type": "string"
        }
      },
      "WorkspaceId": {
        "name": "workspaceId",
        "in": "path",
        "required": true,
        "schema": {
          "type": "string",
          "pattern": "^w[0-9a-f]{24}$"
        }
      }
    },
    "responses": {
      "BadRequest": {
        "description": "Bad request",
        "content": {
          "application/json": {
            "schema": {
              "$ref": "#/components/schemas/ErrorResponse"
            }
          }
        }
      },
      "NotFound": {
